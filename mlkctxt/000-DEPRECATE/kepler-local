#!/bin/bash

# ---------------------------
#
# Common settings for all contexts.
#
# ---------------------------
#
# Prefix variables with MLKC_.
#
# ---------------------------

# -----
# PostGIS
# -----
export MLKC_CELL_RAW_DATA_NETWORK=
export MLKC_CELL_RAW_DATA_HOST=$MLKC_SYSTEM_KEPLER_HOST
export MLKC_CELL_RAW_DATA_PG_PORT=$MLKC_SYSTEM_KEPLER_PG_PORT
export MLKC_CELL_RAW_DATA_PG_USER=$MLKC_SYSTEM_KEPLER_PG_USER
export MLKC_CELL_RAW_DATA_PG_PASS=$MLKC_SYSTEM_KEPLER_PG_PASS
export MLKC_CELL_RAW_DATA_PG_DOCKER_EXTERNAL_PORT=5632

export MLKC_CELL_RAW_DATA_CELL_MASTER_USER=$MLKC_SYSTEM_KEPLER_PG_USER_CELL_MASTER
export MLKC_CELL_RAW_DATA_CELL_MASTER_PASS=$MLKC_SYSTEM_KEPLER_PG_PASS_CELL_MASTER
export MLKC_CELL_RAW_DATA_CELL_READONLY_USER=$MLKC_SYSTEM_KEPLER_PG_USER_CELL_READONLY
export MLKC_CELL_RAW_DATA_CELL_READONLY_PASS=$MLKC_SYSTEM_KEPLER_PG_PASS_CELL_READONLY
